{
  "name": "business-canvas-gpt-server",
  "version": "1.0.0",
  "description": "Api Rest para gerar um Business Canvas a partir de respostas sobre o empreendimento do usu√°rio. Utilizando TDD, DDD, Clean Architecture e TypeScript",
  "main": "server.ts",
  "author": "Lucas Marques",
  "license": "ISC",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "up": "npm run build && docker-compose up",
    "build": "rimraf dist && tsc -p tsconfig-build.json && npm run copy-schema",
    "copy-schema": "ncp src/infra/db/prisma/schema dist/infra/db/prisma/schema",
    "dev": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.ts",
    "test:verbose-unit": "jest --passWithNoTests --runInBand -c jest-unit-config.ts",
    "test:verbose-integration": "jest --passWithNoTests --runInBand -c jest-integration-config.ts",
    "test:integration": "npm test -- --watch -c jest-integration-config.ts",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:ci": "npm test -- --coverage",
    "dev-seed:questions": "ts-node src/main/seeds/add-and-reply-questions-seed.ts",
    "dev-seed:components": "ts-node src/main/seeds/add-all-components-seed.ts",
    "seed:questions": "node dist/main/seeds/add-and-reply-questions-seed.js",
    "seed:components": "node dist/main/seeds/add-all-components-seed.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "mockdate": "^3.0.5",
    "ncp": "^2.0.0",
    "pg": "^8.11.3",
    "prisma": "^5.5.2",
    "prismock": "^1.23.0",
    "rimraf": "^5.0.5",
    "sucrase": "^3.34.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "openai": "^4.19.0",
    "uuid": "^9.0.1"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "prisma": {
    "schema": "./dist/infra/db/prisma/schema/schema.prisma"
  }
}
